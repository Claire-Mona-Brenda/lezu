<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dp_284"
    android:layout_height="wrap_content"
    android:background="@drawable/shape_user"
    android:orientation="vertical"
    android:gravity="center_horizontal">
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/do_failed"
    android:textSize="@dimen/sp_18"
    android:textColor="@color/text_black"
    android:typeface="sans"
    android:textStyle="bold"
    android:layout_marginTop="@dimen/dp_24"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/no_renzheng_tips"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/text_black"
        android:layout_marginTop="@dimen/dp_25"/>
    <TextView
        android:id="@+id/pop_renzheng_tv_to_renzheng"
        android:layout_width="@dimen/dp_250"
        android:layout_height="@dimen/dp_40"
        android:background="@drawable/shape_main"
        android:layout_marginTop="@dimen/dp_33"
        android:layout_marginBottom="@dimen/dp_20"
        android:gravity="center"
        android:textColor="@color/color_ffffff"
        android:textSize="@dimen/sp_14"
        android:text="@string/to_renzheng"/>
</LinearLayout>