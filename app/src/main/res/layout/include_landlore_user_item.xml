<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_bg"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
       android:background="@color/color_ffffff">
        <RelativeLayout
            android:id="@+id/tv_rent_people"
            style="@style/rl_main_user_top_drawable">
            <ImageView
                android:id="@+id/img_rent_people"
                style="@style/img_main_user_top_drawable"
                android:src="@mipmap/zuke_icon" />
            <TextView
                style="@style/tv_main_user_top_drawable"
                android:text="@string/tenanter_title"
                android:layout_below="@+id/img_rent_people" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tv_bill"
            style="@style/rl_main_user_top_drawable">
            <ImageView
                android:id="@+id/img_bill"
                style="@style/img_main_user_top_drawable"
                android:src="@mipmap/zhangdan_icon" />
            <TextView
                style="@style/tv_main_user_top_drawable"
                android:text="@string/bill_title"
                android:layout_below="@+id/img_bill"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/tv_collection"
            style="@style/rl_main_user_top_drawable">
            <ImageView
                android:id="@+id/img_collection"
                style="@style/img_main_user_top_drawable"
                android:src="@mipmap/cuizhang_icon" />
            <TextView
                style="@style/tv_main_user_top_drawable"
                android:text="@string/bill_collection"
                android:layout_below="@+id/img_collection"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/tv_message"
            style="@style/rl_main_user_top_drawable">
            <ImageView
                android:id="@+id/img_message"
                style="@style/img_main_user_top_drawable"
                android:layout_width="@dimen/dp_70"
                android:src="@mipmap/xiaoxi_icon" />
            <TextView
                style="@style/tv_main_user_top_drawable"
                android:text="@string/message"
                android:layout_below="@+id/img_message"/>

            <TextView
                android:id="@+id/tv_message_sum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignRight="@+id/img_message"
                android:layout_alignTop="@+id/img_message"
                android:background="@drawable/shape_ren_point"
                android:gravity="center"
                android:paddingLeft="@dimen/dp_5"
                android:paddingRight="@dimen/dp_5"
                android:text="···"
                android:textColor="@color/color_ffffff"
                android:textSize="@dimen/style_text_tips" />
        </RelativeLayout>



    </LinearLayout>
