<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_ffffff">

    <include layout="@layout/include_title_text" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/style_line"
        android:layout_below="@+id/lin_title"/>

    <TextView
        android:id="@+id/tv_tips_order_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/text_black"
        android:text="@string/title_order_info"
        android:typeface="sans"
        android:textStyle="bold"
        android:layout_below="@+id/lin_title"
        android:layout_marginStart="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_5"
        android:padding="@dimen/dp_10"/>
    <TextView
        android:id="@+id/activity_pws_order_details_tv_copy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/tv_tips_order_info"
        android:layout_marginEnd="@dimen/dp_16"
        android:padding="@dimen/dp_10"
        android:text="@string/easy_copy"
        android:textColor="@color/selector_text_color_white_blue"
        android:textSize="@dimen/sp_16" />
    <TextView
        android:id="@+id/tv_tips_cardId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/card_number"
        android:textColor="@color/text_gray_666666"
        android:textSize="@dimen/sp_14"
        android:layout_below="@+id/tv_tips_order_info"
        android:layout_marginStart="@dimen/dp_22"
        android:layout_marginTop="@dimen/dp_5"/>

    <TextView
        android:id="@+id/activity_pws_order_details_tv_cardId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/tv_tips_cardId"
        android:layout_marginEnd="@dimen/dp_26"
        android:textColor="@color/color_000000"
        android:textIsSelectable="true"
        android:textSize="@dimen/sp_14" />
    <TextView
        android:id="@+id/tv_tips_pwd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/card_pwd"
        android:textColor="@color/text_gray_666666"
        android:textSize="@dimen/sp_14"
        android:layout_below="@+id/tv_tips_cardId"
        android:layout_alignStart="@+id/tv_tips_cardId"
        android:layout_marginTop="@dimen/dp_15" />

    <TextView
        android:id="@+id/activity_pws_order_details_tv_pwd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/tv_tips_pwd"
        android:layout_marginEnd="@dimen/dp_26"
        android:textColor="@color/color_000000"
        android:textIsSelectable="true"
        android:textSize="@dimen/sp_14"  />

    <TextView
        android:id="@+id/tv_tips_start_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/start_rent"
        android:textColor="@color/text_gray_666666"
        android:textSize="@dimen/sp_14"
        android:layout_below="@+id/tv_tips_pwd"
        android:layout_alignStart="@+id/tv_tips_pwd"
        android:layout_marginTop="@dimen/dp_15"  />

    <TextView
        android:id="@+id/activity_pws_order_details_tv_startTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/tv_tips_start_time"
        android:layout_marginEnd="@dimen/dp_26"
        android:textColor="@color/color_000000"
        android:textIsSelectable="true"
        android:textSize="@dimen/sp_14" />

    <TextView
        android:id="@+id/tv_tips_end_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/end_rent"
        android:textColor="@color/text_gray_666666"
        android:textSize="@dimen/sp_14"
        android:layout_below="@+id/tv_tips_start_time"
        android:layout_alignStart="@+id/tv_tips_start_time"
        android:layout_marginTop="@dimen/dp_15"  />

    <TextView
        android:id="@+id/activity_pws_order_details_tv_endTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/tv_tips_end_time"
        android:layout_marginEnd="@dimen/dp_26"
        android:textColor="@color/color_000000"
        android:textIsSelectable="true"
        android:textSize="@dimen/sp_14" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_4"
            android:background="@drawable/shape_line_dashed"
            android:layout_below="@+id/tv_tips_end_time"
            android:layout_marginTop="@dimen/dp_30"/>

    <TextView
        android:id="@+id/tv_tips_room_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/text_black"
        android:text="@string/title_room_info"
        android:typeface="sans"
        android:textStyle="bold"
        android:layout_below="@+id/tv_tips_end_time"
        android:layout_marginStart="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_60"
        android:padding="@dimen/dp_10"/>

    <TextView
        android:id="@+id/tv_tips_room_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/card_room_name"
        android:textColor="@color/text_gray_666666"
        android:textSize="@dimen/sp_14"
        android:layout_below="@+id/tv_tips_room_info"
        android:layout_alignStart="@+id/tv_tips_room_info"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_15" />

    <TextView
        android:id="@+id/activity_pws_order_details_tv_houesId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/tv_tips_room_name"
        android:layout_marginEnd="@dimen/dp_26"
        android:textColor="@color/color_000000"
        android:textIsSelectable="true"
        android:textSize="@dimen/sp_14"  />

    <TextView
        android:id="@+id/tv_tips_room_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/card_room_no"
        android:textColor="@color/text_gray_666666"
        android:textSize="@dimen/sp_14"
        android:layout_below="@+id/tv_tips_room_name"
        android:layout_alignStart="@+id/tv_tips_room_name"
        android:layout_marginTop="@dimen/dp_15" />

    <TextView
        android:id="@+id/activity_pws_order_details_tv_houes_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/tv_tips_room_type"
        android:layout_marginEnd="@dimen/dp_26"
        android:textColor="@color/color_000000"
        android:textIsSelectable="true"
        android:textSize="@dimen/sp_14"  />

    <TextView
        android:id="@+id/tv_tips_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/card_room_address"
        android:textColor="@color/text_gray_666666"
        android:textSize="@dimen/sp_14"
        android:layout_below="@+id/tv_tips_room_type"
        android:layout_alignStart="@+id/tv_tips_room_type"
        android:layout_marginTop="@dimen/dp_15" />

    <TextView
        android:id="@+id/activity_pws_order_details_tv_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="end"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/tv_tips_address"
        android:layout_toEndOf="@+id/tv_tips_address"
        android:layout_marginStart="@dimen/dp_60"
        android:layout_marginEnd="@dimen/dp_26"
        android:textColor="@color/color_000000"
        android:textIsSelectable="true"
        android:textSize="@dimen/sp_14"  />
</RelativeLayout>